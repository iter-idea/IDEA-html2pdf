{
  "name": "idea-html2pdf",
  "version": "2.0.8",
  "description": "IDEA helper for generating a PDF from HTML contents",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts",
    "compile": "tsc --build",
    "docs": "typedoc index.ts --customCss ./docs.style.css",
    "build": "./build.sh",
    "publishPackage": "./build.sh && npm publish"
