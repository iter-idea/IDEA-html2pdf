  "bugs": {
    "url": "https://github.com/uatisdeproblem/IDEA-AWS/issues"
  },
  "homepage": "https://uatisdeproblem.github.io/IDEA-AWS",
  "dependencies": {
    "handlebars": "^4.7.8",
    "idea-toolbox": "^7.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.388.0",
    "@aws-sdk/client-s3": "^3.388.0",
    "@aws-sdk/lib-storage": "^3.388.0",
    "@aws-sdk/s3-request-presigner": "^3.388.0",
